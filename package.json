{
  "name": "symbol-jump-and-select",
  "displayName": "Symbol Jump and Select",
  "description": "Go to filtered symbols (with optional selection).",
  "preview": true,
  "version": "0.1.0",
  "publisher": "ArturoDent",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArturoDent/symbol-jump-and-select"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "symbol",
    "jump",
    "move",
    "cursor",
    "select"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "browser": "./out/extension.js",
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "%APP_SETTINGS_HOME%/keybindings.json",
        "url": "./schemas/keybindings.schema.jsonc"
      },
      {
        "fileMatch": "%APP_SETTINGS_HOME%/profiles/*/keybindings.json",
        "url": "./schemas/keybindings.schema.jsonc"
      }
    ],
    "configuration": [
      {
        "title": "Symbol: Jump-and-Select",
        "additionalProperties": false,
        "properties": {
          "symbol-jump-and-select.useTypescriptCompiler": {
            "type": "boolean",
            "scope": "resource",
            "default": "true",
            "markdownDescription": "Whether to use the more resource-intensive typescript compiler (`tsc`) to produce more detailed symbol entries."
          }
        }
      }
    ],
    "commands": [
      {
        "command": "symbol-jump-and-select.showQuickPick",
        "title": "Open a quickpick of filtered symbols.",
        "category": "Symbol: Jump/Select"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "fuzzysort": "^3.1.0",
    "typescript": "^5.9.2"
  }
}